<h3>New Shipping Method</h3>

<p>An example Shipping Method could be "UPS 3-5 day"</p>

<%= form_for(@shipping_method, :url => admin_config_shipping_methods_path) do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= f.submit t("button_create"), :class => "button" %>
  <%= link_to  t("button_back"), admin_config_shipping_methods_path, :class => "secondary button" %>
<% end %>

