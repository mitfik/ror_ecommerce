<h1>Editing account</h1>


<%= form_for( @account, :url => admin_config_account_path(@account)) do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= f.submit t("button_save"), :class => "button" %>
  <%= link_to 'Back', admin_config_accounts_path, :class => 'secondary button' %>
<% end %>

