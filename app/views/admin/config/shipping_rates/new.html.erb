<h3>New Shipping Rate</h3>

<%= form_for(@shipping_rate, :url => admin_config_shipping_rates_path) do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= f.submit t("button_create"), :class => "button" %>
  <%= link_to t("button_back"), admin_config_shipping_rates_path, :class => "secondary button" %>
<% end %>

