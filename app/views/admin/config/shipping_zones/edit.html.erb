<h3>Editing Shipping Zone - <%= @shipping_zone.name %></h3>

<%= form_for(@shipping_zone, :url => admin_config_shipping_zone_path(@shipping_zone)) do |f| %>
  <%= render :partial =>  'form', :locals => {:f => f} %>
  <%= f.submit t("button_save"), :class => "button" %>
  <%= link_to t("button_back"), admin_config_shipping_zones_path, :class => 'secondary button' %>
<% end %>

