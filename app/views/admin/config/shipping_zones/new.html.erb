<h1>New Shipping Zone</h1>


<%= form_for(@shipping_zone, :url => admin_config_shipping_zones_path()) do |f| %>
  <%= render :partial =>  'form', :locals => {:f => f} %>
  <%= f.submit t("button_create"), :class => "button" %>
  <%= link_to t("button_back"), admin_config_shipping_zones_path, :class => 'secondary button' %>
<% end %>

