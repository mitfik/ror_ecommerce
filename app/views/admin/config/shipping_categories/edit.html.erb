<h3>Editing Shipping Category <%= @shipping_category.name %> </h3>

<%= form_for(@shipping_category, :url => admin_config_shipping_category_path(@shipping_category)) do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= f.submit t("button_save"), :class => "button" %>
  <%= link_to t("button_back"), admin_config_shipping_categories_path, :class => 'secondary button' %>
<% end %>

